@model IEnumerable<WebAppSemana06.Models.TemporalVenta>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<html>
    <head></head>
    <body>
        <h3 class="align-items-center alert-primary">Carrito de compras</h3>
        <table class="table table-bordered">
            <tr>
                <td>Codigo</td>
                <td>Descripcion</td>
                <td>Precio</td>
                <td>Cantidad</td>
            </tr>
            <tbody>
                @foreach(var item in Model)
                {
                    <tr>
                        <td>@item.codigo</td>
                        <td>@item.descripcion</td>
                        <td>@item.precio</td>
                        <td>@item.cantidad</td>
                    </tr>
                }
        </tbody>
        </table>
    </body>
</html>
