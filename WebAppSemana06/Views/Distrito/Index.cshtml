@model IEnumerable<WebAppSemana06.Models.TbDistrito>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<html>
    <head></head>
    <body>
        <h2><a asp-action="Nuevo">Nuevo Distrito</a></h2>
        <h3 class="alert-primary">Listado de distritos</h3>
        <table class="table table-bordered">
            <tr>
                <td>Codigo</td>
                <td>Nombre del distrito</td>
                <td>Eliminar</td>
                <td>Modificar</td>
            </tr>
            <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.CodDis</td>
                    <td>@item.NomDis</td>
                    <td><a asp-action="Eliminar" asp-route-id="@item.CodDis">Eliminar</a></td>
                    <td><a asp-action="Modificar" asp-route-id="@item.CodDis">Modificar</a></td>
                </tr>
            }
        </tbody>
        </table>
    </body>
</html>
